{
  "setupTasks": [
    {
      "name": "Install backend dependencies",
      "command": "cd photo-sharing-backend && npm install"
    },
    {
      "name": "Install frontend dependencies",
      "command": "cd photo-sharing-v1 && npm install"
    }
  ],
  "tasks": {
    "run back end": {
      "name" : "run back end",
      "command": "cd photo-sharing-backend && npm run dev",
      "runAtStart": true,
      "preview": {
        "port": 8080
      }
    },
    "run front end": {
      "name":"run front end",
      "command": "cd photo-sharing-v1 && npm run start",
      "runAtStart": true,
      "preview": {
        "port": 3000
      }
    }
  }
}
